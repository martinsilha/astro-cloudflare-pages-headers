{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/**/*",
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve"
  }
}
