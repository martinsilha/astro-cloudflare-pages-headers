{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/**/*",
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "demo"
  ],
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "preserve",
    "skipLibCheck": true
  }
}
